		SELECT  kaoqindaka.* FROM kaoqindaka  kaoqindaka 	
 		<where> 1=1 ${ew.sqlSegment}</where>
	</select>

    <select id="selectListView"
		resultType="com.entity.view.KaoqindakaView" >

		SELECT  kaoqindaka.* FROM kaoqindaka  kaoqindaka 	        
        <where> 1=1 ${ew.sqlSegment}</where>
	</select>
	
	<select id="selectView"
		resultType="com.entity.view.KaoqindakaView" >
